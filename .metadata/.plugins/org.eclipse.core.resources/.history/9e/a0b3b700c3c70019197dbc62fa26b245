package exam_printpage;

public class ScoreOXMain {
	public static void main(String[] args) {
		int[] dap1 = {1,2,1,3,4};
		int[] dap2 = {1,2,1,3,4};
		int[] dap3 = {1,2,1,3,4};
		int[] dap4 = {1,2,1,3,4};
		int[] dap5 = {1,2,1,3,4};
		
		System.out.printf(" * * 시험결과 * * \n\n");
		System.out.printf("-------------------------------------------------\n");
		System.out.printf("번호\t이름\t1\t2\t3\t4\t5\t점수\t등수\n");
		System.out.printf("-------------------------------------------------\n");
		
		System.out.printf("전체 맞은 개수 = %2d");
	}
}
