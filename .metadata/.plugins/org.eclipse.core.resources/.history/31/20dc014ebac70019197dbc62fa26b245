package exam_class_relation;

public class Circle {
	private int radius;;
	private Point center;

	//»ý¼ºÀÚ
	public Circle() {
		radius = 0;
		p2 = new Point(0, 5);
	}

	public Circle(Point p1, Point p2) {
		this.p1 = p1;
		this.p2 = p2;
	}

	public String toString() {
		return "Circle return : p1=" + p1.toString() + ", p2=" + p2.toString();
	}

}
