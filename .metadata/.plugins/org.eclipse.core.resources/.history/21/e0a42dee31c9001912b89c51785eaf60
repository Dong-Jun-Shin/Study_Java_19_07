package exam_super;

class ParentClass{
	protected int data;
	
	public ParentClass() {
		data=100;
	}
	
	public void print() {
		System.out.println("슈퍼 클래스의 print() 메소드");
	}
}

class ChildClass extends ParentClass{
	protected int data;
	
	public ChildClass() {
		data = 200;
	}
	
}

public class ChildClassTest {
	public static void main(String[] args) {
		ChildClass obj = new ChildClass();
		obj.print();
	}
}
