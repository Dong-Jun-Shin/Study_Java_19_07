package exam_printpage;

public class ScoreOXMain {
	public static void main(String[] args) {
		int[] dap1 = {1,2,1,3,4};
		int[] dap2 = {1,1,1,1,1};
		int[] dap3 = {1,2,1,3,1};
		int[] dap4 = {1,1,1,3,1};
		int[] dap5 = {1,4,2,5,4};
		
		ScoreOX s1 = new ScoreOX(1, "홍길동", dap1);
		ScoreOX s2 = new ScoreOX(2, "김철수", dap2);
		ScoreOX s3 = new ScoreOX(3, "이진희", dap3);
		ScoreOX s4 = new ScoreOX(4, "조현민", dap4);
		ScoreOX s5 = new ScoreOX(5, "최현정", dap5);
		
		System.out.printf(" * * 시험결과 * * \n\n");
		System.out.printf("-------------------------------------------------\n");
		System.out.printf("번호\t이름\t1\t2\t3\t4\t5\t점수\t등수\n");
		System.out.printf("-------------------------------------------------\n");
		
		System.out.printf("전체 맞은 개수 = %2d");
	}
}
