package exam_class;

public class SongTest {
	public static void main(String[] args) {
		Song song = new Song("Dansing Queen", "ABBA", "Arrival", 
				Song.setComposer("Benny Andersson", "Bjorn Ulvaeus"),
				1977, 2);
	
		song.show();

		Song song = new Song("Dansing Queen", "ABBA", "Arrival", 
				new String[] {"Benny Andersson", "Bjorn Ulvaeus"},
				1977, 2);
		
		song.show();
		
		
		
