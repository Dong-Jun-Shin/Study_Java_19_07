package exam_printpage;

public class cry {
	private int 발갯수;
	
	public void 셋발갯수(int 발) {
		this.발갯수 = 발;
	}
	public void cry() {
		System.out.println("나는 운다. 이렇게");
	}
}

class cat extends cry{
	public void cryCat() {
		System.out.println("mumu~");
	}
}

class dog extends cry{
	public void cryDog() {
		System.out.println("walwal~");
	}
}
