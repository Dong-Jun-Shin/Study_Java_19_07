<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="1200.0"
	spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="exam_URLConnection.ChungnamController">
	<children>
		<TableView prefHeight="680.0" prefWidth="1200.0" fx:id="cultureTourTableView" onMouseClicked="#cultureTourTableView">
			<columns>
				<TableColumn prefWidth="75.0" text="번호" />
				<TableColumn prefWidth="75.0" text="지역명" />
				<TableColumn prefWidth="75.0" text="제목" />
				<TableColumn prefWidth="75.0" text="부제목" />
				<TableColumn prefWidth="200.0" text="주소" />
				<TableColumn prefWidth="75.0" text="경도" />
				<TableColumn prefWidth="75.0" text="위도" />
				<TableColumn prefWidth="200.0" text="전화번호" />
				<TableColumn prefWidth="200.0" text="URL" />
				<TableColumn prefWidth="200.0" text="상세설명" />
				<TableColumn prefWidth="200.0" text="썸네일 이미지" />
				<TableColumn prefWidth="75.0" text="등록일" />
				<TableColumn prefWidth="75.0" text="축제기간" />
				<TableColumn prefWidth="200.0" text="아이템 이미지" />
			</columns>
		</TableView>
		<Button  mnemonicParsing="false"
			onAction="#btnClose" prefHeight="23.0" prefWidth="146.0" text="닫기" />
	</children>
	<padding>
		<Insets bottom="10.0" />
	</padding>
</VBox>
